package CauDieuKien;

import java.util.Scanner;

public class TracNghiemChuan {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
System.out.println("Phat co dep trai khong");
System.out.println("A.co   B.khong  C.tam duoc  D.oppa");
Scanner scan=new Scanner(System.in);
String s=scan.nextLine();
switch (s) {
case s='a':System.out.println("Hoi dung nhung chua chinh xac");
break;
case s='b':System.out.println("Ngu");
break;
case s='c':System.out.println("sai");
break;
case s='d':System.out.println("dung chinh xac, ban qua thong minh");
break;
}
	}

}
