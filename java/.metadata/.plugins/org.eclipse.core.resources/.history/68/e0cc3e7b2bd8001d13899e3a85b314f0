
public class KieuDuLieu {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Kieu so nguyen
		byte b=30;
		short s=200;
		int i=30000000;
		//Kieu so thuc
		float f=4.354f;
		double d=4.23;
		//Kieu ki tu
		char v= 'A';
		//Kieu TRUE FALSE
		boolean l= true;
		boolean n= false;
		//xuat ra
		System.out.println(b);
		System.out.println(s);
		System.out.println(i);
		System.out.println(v);
		System.out.println(l);
		float o=575.85f;
		System.out.println(o);
		float traiDepKhoaiTo=50.6f;
		System.out.println(traiDepKhoaiTo);
		System.out.println("do dai cua tk e toi la" traiDepKhoaiTo);
		
		
	}

}
