
public class Chuoi {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="Xin Chao ";
		String t="Kinh nghiem";
		String ghep=s+t;
		System.out.println(s);
		System.out.println(t);
		System.out.println(ghep);
		int i=10;
		System.out.println(ghep+i);
		int x=300;
		int y=250;
		System.out.println(x+y);
				

	}

}
