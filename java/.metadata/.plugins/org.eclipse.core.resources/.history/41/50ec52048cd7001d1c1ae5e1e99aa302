
public class KieuDuLieu {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Kieu so nguyen
		byte b=30;
		short s=20000;
		int i=3000000000000000000;
		//Kieu so thuc
		float f=4.354f;
		double d=4.23;
		//Kieu ki tu
		char v= 'ABc';
		//Kieu TRUE FALSE
		boolean l= true;
		boolean n= false;
		//xuat ra
		System.out.println();
		
		
	}

}
